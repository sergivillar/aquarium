{
    "name": "server",
    "version": "1.0.0",
    "description": "GraphQL server",
    "main": "index.js",
    "scripts": {
        "initdb": "docker-compose up -d",
        "start": "nodemon index.ts --exec babel-node --extensions '.ts' index.ts",
        "check-types": "tsc",
        "pretest": "NODE_ENV=test babel-node --extensions '.ts' ./src/utils/setup-test.ts",
        "test": "NODE_ENV=test jest --runInBand",
        "lint": "node_modules/.bin/tslint -p ../../tsconfig.json",
        "generate-types": "gql-gen --require ts-node/register --schema ./src/graphql/index.ts --template graphql-codegen-typescript-template --out ./src/typings/generated.d.ts"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.1.0",
        "@babel/node": "^7.0.0",
        "@babel/plugin-proposal-class-properties": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@babel/preset-typescript": "^7.0.0",
        "@types/bcrypt": "^2.0.0",
        "@types/dotenv": "^4.0.3",
        "@types/express": "^4.16.0",
        "@types/graphql": "^14.0.3",
        "@types/jest": "^23.3.2",
        "@types/jsonwebtoken": "^7.2.8",
        "@types/passport": "^0.4.6",
        "@types/passport-jwt": "^3.0.1",
        "@types/passport-local": "^1.0.33",
        "@types/sequelize": "^4.27.25",
        "@types/supertest": "^2.0.6",
        "babel-core": "^7.0.0-bridge",
        "babel-jest": "^23.6.0",
        "graphql-code-generator": "^0.13.0",
        "graphql-codegen-typescript-template": "^0.13.0",
        "graphql-tools": "^3.1.1",
        "jest": "^23.6.0",
        "nodemon": "^1.18.3",
        "supertest": "^3.3.0",
        "ts-node": "^7.0.1",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.15.0",
        "tslint-eslint-rules": "^5.4.0",
        "typescript": "^3.0.3"
    },
    "dependencies": {
        "apollo-server-express": "^2.0.6",
        "bcrypt": "^3.0.0",
        "dataloader": "^1.4.0",
        "dotenv": "^6.0.0",
        "express": "^4.16.3",
        "graphql": "^14.0.2",
        "jsonwebtoken": "^8.3.0",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pg": "^7.4.3",
        "sequelize": "^4.38.1",
        "validator": "^10.9.0"
    },
    "jest": {
        "moduleDirectories": [
            "./node_modules",
            "./src"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx"
        ],
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
        },
        "testRegex": "(/__tests__/.*|\\.(test))\\.(ts|tsx|js)$",
        "verbose": false,
        "testEnvironment": "node"
    }
}
