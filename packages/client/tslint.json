{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended", "tslint-eslint-rules", "tslint-config-prettier", "tslint-react"],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "member-access": false,
        "no-console": [true, "log"],
        "jsx-no-multiline-js": false,
        "interface-name": false,
        "jsx-no-lambda": false,
        "jsx-boolean-value": false
    },
    "rulesDirectory": []
}
